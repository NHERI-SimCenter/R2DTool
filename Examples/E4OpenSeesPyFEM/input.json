{
  "Name": "E4 - OpenSeesPy FEM",
  "Author": "Stevan Gavrilovic",
  "units": {
      "force": "kips",
      "length": "in",
      "time": "sec"
   },
   "outputs": {
    "EDP": true,
    "DM": true,
    "DV": true,
    "every_realization": true
  },
  "Applications": {
    "Hazard": {
        "Application": "UserInputGM",
        "ApplicationData": {
	    "eventFile": "/records/EventGrid.csv",
	    "motionDir": "/records"
	}
    },
    "Building": {
      "Application": "CSV_to_BIM",
      "ApplicationData": {
        "filter": "0,14,35,57,76",
        "buildingSourceFile":"UniversityVillageAlbany.csv"
      }
    },
    "RegionalMapping": {
      "Application": "NearestNeighborEvents",
      "ApplicationData": {
        "filenameEVENTgrid": "records/EventGrid.csv",
        "samples": 5,
        "neighbors": 4
      }
    },
    "Events": [{
      "EventClassification": "Earthquake",
      "Application": "SimCenterEvent",
      "ApplicationData": {}
    }],
    "Modeling": {
      "Application": "OpenSeesPyInput",
      "ApplicationData": {
        "mainScript": "cantilever_light.py",
        "modelPath": "model/",
        "ndm": 3,
        "dofMap": "1,2,3"
      }
    },
    "EDP": {
      "Application": "StandardEarthquakeEDP_R",
      "ApplicationData": {}
    },
    "Simulation": {
       "Application": "OpenSeesPy-Simulation",
       "ApplicationData": {}
    },
    "UQ": {
       "Application": "Dakota-UQ",
       "ApplicationData": {
          "method": "LHS",
          "samples": 5,
          "type": "UQ",
          "concurrency": 1,
          "keepSamples": true
       }
    },
    "DL": {
            "Application": "pelicun",
            "ApplicationData": {
                "DL_Method": "HAZUS MH EQ",
                "Realizations": 5,
                "detailed_results": false,
                "log_file": true,
                "coupled_EDP": true,
                "event_time": "off",
                "ground_failure": false
            }
        }
    }
}
